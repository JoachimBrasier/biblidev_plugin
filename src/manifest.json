{
  "manifest_version": 2,
  "name": "Biblidev",
  "version": "0.0.0",
  "description": "Naviguez rapidement parmi les ressources que vous avez partagées, aimées et notées",
  "icons": {
    "16": "assets/icons/16x16.png",
    "48": "assets/icons/48x48.png",
    "96": "assets/icons/96x96.png",
    "128": "assets/icons/128x128.png"
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/16x16.png",
      "32": "assets/icons/32x32.png",
      "48": "assets/icons/48x48.png",
      "128": "assets/icons/128x128.png"
    },
    "default_title": "Biblidev",
    "default_popup": "popup.html"
  }
}